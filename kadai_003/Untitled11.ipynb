        "# NumPyをインポートするコードを記述する\n",
        "import numpy as np\n",
        "# AとBの行列を格納する多次元配列のコードを記述する\n",
        "A = np.array([[0, 1], [2, 3], [4, 5]])\n",
        "B = np.array([[0, 1, 2, 3], [4, 5, 6, 7]])\n",
        "# 行列の積の計算をするコードを記述する\n",
        "result = np.dot(A, B)\n",
        "# Step3の計算結果を表示するコードを記述する\n",
        "print(result)\n",
        "# 行列の要素の最大値を求めるコードを記述する\n",
        "result.max()\n"
      ],
